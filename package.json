  "devDependencies": {
    "tslint": "^5.20.0",
    "tslint-config-standard": "^8.0.1",
    "typescript": "^3.6.4"
  },
  "dependencies": {
    "@aws-cdk/aws-budgets": "^1.14.0",
    "@aws-cdk/aws-certificatemanager": "^1.14.0",
    "@aws-cdk/aws-ec2": "^1.14.0",
    "@aws-cdk/aws-elasticloadbalancingv2": "^1.14.0",
    "@aws-cdk/aws-iam": "^1.14.0",
